Bool : * = #[]{
  #true{} : Bool
  #false{} : Bool
}

Nat : * = #[]{
  #zero{} : Nat
  #succ{ pred: Nat } : Nat
}


IsTrue : ∀(b: Bool) * = λb #[b]{
  #indeed{} : (IsTrue #true{})
}

Equal : ∀(T: *) ∀(a: T) ∀(b: T) * = λT λa λb #[a b]{
  #refl{} : (Equal T a a)
}

rewrite
: ∀(T: *)
  ∀(a: T)
  ∀(b: T)
  ∀(e: (Equal T a b))
  ∀(P: ∀(x: A) *)
  ∀(x: (P a))
  (P b)
= λT λa λb ?a[T,a,b]

foo
: U48
= 45

example = rewrite
