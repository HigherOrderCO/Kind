Hex.parser : Parser(Nat)
  Parser {
    Parser.maybe!(Parser.text("0x"))
    Parser.bind(List<Nat>, Nat, Parser.many1(Nat, Hex.parser.char_hex), (xs)
    Parser.pure!(
    case xs {
      nil : 0
      cons : List.fold_right(Nat, Nat, xs.head, Function.flip(_, _ , _Nat.append_2(3)), xs.tail)
    }))
  }
