Kind.Defs.core(defs: Kind.Defs): String
  let result = "";
  let result = for defn in BitsMap.values!(defs):
    case defn {
      new: 
        case defn.stat {
          done:
            let name = defn.name;
            let term = Kind.Core.show(defn.term, 0, []);
            let type = Kind.Core.show(defn.type, 0, []);
            String.flatten([result, name, " : ", type, " = ", term, ";\n"]),
          wait: result,
          init: result,
          fail: result,
      }
    }
  result
