Kind.Synth.many(names: List<String>, defs: Kind.Defs): IO<Kind.Defs>
  case names {
    nil: IO {
      return defs;
    },
    cons: IO {
      get new_defs = Kind.Synth.one(names.head, defs);
      case new_defs {
        none: Kind.Synth.many(names.tail, defs),
        some: Kind.Synth.many(names.tail, new_defs.value),
      }
    },
  }
