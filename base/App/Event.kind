type App.Event {
  init(
    time: U64,
    addr: String,
    screen: Pair<U32,U32>,
    mouse: Pair<U32,U32>,
  )
  tick(
    time: U64,
    screen: Pair<U32,U32>,
    mouse: Pair<U32,U32>,
  )
  xkey(
    time: U64,
    down: Bool,
    code: U16,
  )
  post(
    time: U64,
    room: String,
    addr: String,
    data: String,
  )
}
