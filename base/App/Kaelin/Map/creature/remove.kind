App.Kaelin.Map.creature.remove(
  coord: App.Kaelin.Coord
  map: App.Kaelin.Map
): App.Kaelin.Map

  key = App.Kaelin.Coord.Convert.axial_to_nat(coord)
  tile = NatMap.get!(key, map)
  case tile {
    none:
      map
    some:
      new_tile = tile.value@creature <- none
      NatMap.set!(key, new_tile, map)
  }
