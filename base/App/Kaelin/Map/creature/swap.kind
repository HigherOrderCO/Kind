// Moves the creature  at coordinate "ca" to coordinate "cb"
App.Kaelin.Map.creature.swap(
  ca: App.Kaelin.Coord
  cb: App.Kaelin.Coord
  map: App.Kaelin.Map
): App.Kaelin.Map

  {new_map, creature} = App.Kaelin.Map.creature.pop(ca, map)
  case creature {
    none: 
      map
    some: 
      entity = App.Kaelin.Map.Entity.creature(creature.value)
      App.Kaelin.Map.push(cb, entity, new_map)
  }
