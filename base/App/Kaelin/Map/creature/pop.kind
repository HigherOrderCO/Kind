App.Kaelin.Map.creature.pop(
  coord: App.Kaelin.Coord, 
  map: App.Kaelin.Map
): Pair<App.Kaelin.Map, Maybe<App.Kaelin.Creature>>

  tile = App.Kaelin.Map.get(coord, map)
  case tile {
    none:{map, none}
    some:
      open tile.value as old_tile
      creature = old_tile.creature
      remaining_tile =  App.Kaelin.Tile.new(old_tile.background, none, old_tile.animation)
      new_map = App.Kaelin.Map.set(coord, remaining_tile, map)
      {new_map, creature}
  }

