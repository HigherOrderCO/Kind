// Moves the creature  at coordinate "ca" to coordinate "cb"
App.Kaelin.Map.swap(
  ca: App.Kaelin.Coord
  cb: App.Kaelin.Coord
  map: App.Kaelin.Map
) : App.Kaelin.Map
  {new_map, creature} = App.Kaelin.Map.pop_creature(ca, map)
  case creature {
    none: map
    some: 
    entity = App.Kaelin.Map.Entity.creature(creature.value)
    App.Kaelin.Map.push(cb, entity, new_map)
  }
