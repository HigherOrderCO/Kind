App.Kaelin.App.draw(
  img: VoxBox
): App.Draw<App.Kaelin.State>

(state)
  open state
  stage = state.global@stage
  case stage {
    init: App.Kaelin.Draw.init(state.local@input)
    draft: App.Kaelin.Draw.draft(stage.players, stage.coords, state.local@user)
    planning: App.Kaelin.Draw.game(img, state)
    action: App.Kaelin.Draw.game(img, state)
  }
