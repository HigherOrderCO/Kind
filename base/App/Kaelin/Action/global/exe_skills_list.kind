App.Kaelin.Action.global.exe_skills_list<S: App.State>(
  skills : List<App.Kaelin.CastInfo.global>, 
  glob: App.Kaelin.State.global
): App.Kaelin.State.global
  case skills {
    nil:  
      glob
    cons:
      let new_glob = App.Kaelin.Action.global.exe_skill(
        skills.head@player, 
        skills.head@target_pos, 
        skills.head@key, 
        glob
      )
      App.Kaelin.Action.global.exe_skills_list!(skills.tail, new_glob)
  }