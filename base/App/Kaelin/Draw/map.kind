// function to draw according the state
App.Kaelin.Draw.map(
  img: VoxBox,
  map_to_draw: App.Kaelin.Map,
  state: App.Store<App.Kaelin.State>
): VoxBox

  open state
  open state.local
  open state.global

  let img = App.Kaelin.Draw.state.map(map_to_draw, state.local.cast_info, state.local.env_info, state.local.internal, img)
  let img = App.Kaelin.Draw.state.mouse_ui(state.local.env_info, img)
  img




