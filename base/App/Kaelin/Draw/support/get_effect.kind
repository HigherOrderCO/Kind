// Returns the cast affecting a tile
App.Kaelin.Draw.support.get_effect(
coord: App.Kaelin.Coord,
coord_nat: Nat,
cast_info: Maybe(App.Kaelin.CastInfo)
): Maybe(App.Kaelin.Indicator)

  case cast_info{ 
    none: 
      none
    some: 
      open cast_info.value as cast
      is_in_range = NatSet.has(coord_nat, cast.range)
      if is_in_range then
        some(App.Kaelin.Indicator.blue)
      else
        none
  }
