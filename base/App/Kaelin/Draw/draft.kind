App.Kaelin.Draw.draft: DOM
  heroes = List.map!!(Pair.snd!!, Map.to_list!(App.Kaelin.Resources.heroes))
  DOM.node("div", 
          {}, 
          { "width": "100vw", 
            "height": "100vh", 
            "display": "flex", 
            "flex-direction": "column", 
            "justify-content": "center", 
            "align-items": "center",
            "font-size": "2rem"
          },
          [
            App.Kaelin.Draw.draft.top,
            App.Kaelin.Draw.draft.bottom
            //DOM.node("h1", {}, {"margin-bottom": "50px"}, [DOM.text("Choose a hero")]),
            //DOM.node("div", {}, {"display": "flex", "flex-wrap": "wrap", "justify-content": "center", "max-width": "70%"}, List.map!!(App.Kaelin.Draw.draft.hero_card, heroes)),
          ]
  )




App.Kaelin.Draw.draft.top: DOM
  DOM.node("div", {},
  {"width": "100%",
   "height": "60%",
   "display": "flex"}
   [
     App.Kaelin.Draw.draft.hexes,
     App.Kaelin.Draw.draft.picks,
   ])

App.Kaelin.Draw.draft.hexes: DOM
DOM.node("div", {},
  {"width": "25%",
   "height": "100%",
   "display": "flex"}
   [])

App.Kaelin.Draw.draft.picks: DOM
DOM.node("div", {},
  {"width": "75%",
   "height": "100%",
   "display": "flex"}
  [
    App.Kaelin.Draw.draft.my_hero,
    App.Kaelin.Draw.draft.my_allies
  ])

App.Kaelin.Draw.draft.my_hero: DOM
DOM.node("div", {},
  {"width": "40%",
   "height": "100%",
   "display": "flex",
   "background-color": "cyan"
   "padding": "5%"}
      [App.Kaelin.Draw.draft.hero_card])

App.Kaelin.Draw.draft.my_allies: DOM
DOM.node("div", {},
  {"width": "60%",
   "height": "100%",
   "display": "flex"
   "background-color": "green"}
   [])


App.Kaelin.Draw.draft.bottom: DOM
  DOM.node("div", {},
  {"width": "100%",
   "height": "40%",
   "display": "flex"}
   [])


App.Kaelin.Draw.draft.hero_card: DOM
  DOM.node("div", {},
    {
      "width": "100%"
      "height": "100%"
      "display": "flex"
      "border": "3px solid cyan"
      "border-radius": "5px"
      "flex-direction": "column"
      "justify-content": "flex-start", 
      "align-items": "center",
    }
    [
      DOM.node("div", {}, {}, [DOM.text("hero@name")]),
      DOM.node("div", {}, {"padding-top": "10%"}, [DOM.node("img", 
      {"src": "data:image/gif;base64,"}
      {"width": "192px", "height": "192px"}, [])])
      DOM.node("div", {}, {
      "width": "100%"
      "height": "100%"
      "display": "flex"
      "background-color": "black"} [])
    
    ])






