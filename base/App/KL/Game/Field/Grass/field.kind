App.KL.Game.Field.Grass.field: App.KL.Game.Field
  let name = "Grass"
  let draw = (terrain, indicator)
    case indicator {
      background: App.Kaelin.Assets.tile.green_2
      green: App.Kaelin.Assets.tile.green_1
      red: App.Kaelin.Assets.tile.effect.light_red2
      yellow: App.Kaelin.Assets.tile.effect.light_red2
      blue:App.Kaelin.Assets.tile.effect.dark_blue2
    }
  App.KL.Game.Field.new(name, draw)
