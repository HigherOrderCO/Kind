App.KL.Game.Effect.coord.clamp(
  range:  I32
  center: Hexagonal.Axial
  target: Hexagonal.Axial
): Hexagonal.Axial

  f = I32.to_f64
  distance = Hexagonal.Axial.distance(center, target)
  distance_64 = f(distance)
  range_64 = f(range)

  Hexagonal.Axial.lerp(range_64 / distance_64, center, target)
