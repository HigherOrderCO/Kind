App.KL.Game.Effect.hp.damage_at(
  dmg: I32,
): App.KL.Game.Effect.At<I32>
  (pos)

  App.KL.Game.Effect {
    get center = App.KL.Game.Effect.coord.get_center
    get creature = App.KL.Game.Effect.board.creature.get(center)
    without creature: App.KL.Game.Effect { return +0#32 }
    let debuffer = App.KL.Game.Creature.Status.weaken.total(creature)
    App.KL.Game.Effect.hp.change_at(I32.neg(dmg - debuffer), pos)
  }
