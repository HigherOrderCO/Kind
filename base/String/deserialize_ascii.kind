String.deserialize_ascii.go(hexs: String): String
  case hexs {
    nil:
      ""
    cons: 
      let init = String.take(2, hexs)
      let rest = String.drop(2, hexs)
      let numb = Parser.run!(Parser.hex_nat, "0x" | init) <> 0
      let char = Nat.to_u16(numb)
      if String.eql(init, "00") then
        ""
      else
        Char.to_string(char) | String.deserialize_ascii.go(rest)
  }

String.deserialize_ascii(hexs: String): String
  String.deserialize_ascii.go(String.drop(2, hexs))
