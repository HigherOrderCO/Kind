Parser.ErrorAt.combine(
  a: Maybe<Parser.ErrorAt>,
  b: Maybe<Parser.ErrorAt>,
): Maybe<Parser.ErrorAt>
  case a {
    none: b,
    some: case b {
      none: a,
      some: case a.value {
        new: case b.value {
          new: if Nat.gtn(a.value.idx, b.value.idx) then a else b
        }
      }
    }
  }