// function to draw according the state
Web.Kaelin.Draw.state(img: VoxBox, state: Web.Kaelin.State): VoxBox
  case state {
      game:
        let new_img = Web.Kaelin.Draw.state.background(img, state.map)
        let new_img = Web.Kaelin.Draw.state.players(new_img, state.map)
        new_img
  } default img
