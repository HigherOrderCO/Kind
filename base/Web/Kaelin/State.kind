type Web.Kaelin.State {
  init
  void(
    room: String
  )
  game(
    user: String
    room: String
    players: Map(Web.Kaelin.Player)
    cast_info: Maybe<Web.Kaelin.CastInfo>
    map: Web.Kaelin.Map,
    internal : Web.Kaelin.Internal
    env_info : App.EnvInfo
  )
}

type Web.Kaelin.CastStatus {
  new(
    center_pos: Web.Kaelin.Coord
    target_pos: Web.Kaelin.Coord
    range: Nat
  )
}



