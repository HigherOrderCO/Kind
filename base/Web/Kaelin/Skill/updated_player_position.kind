Web.Kaelin.Skill.updated_player_position(
map: Web.Kaelin.Map, 
hero_coord: Web.Kaelin.Coord, 
target_coord: Web.Kaelin.Coord, 
mod: Web.Kaelin.Skill.Modifier.position, 
value: I32
): Web.Kaelin.Map
  {origin, destination} = case mod {
  move_to: {hero_coord, target_coord}
  }
    Web.Kaelin.Map.swap(0, origin, destination, map)

