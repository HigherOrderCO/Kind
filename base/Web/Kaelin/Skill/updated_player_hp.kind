Web.Kaelin.Skill.updated_player_hp(
player: Web.Kaelin.Player, 
modifier: Web.Kaelin.Skill.Modifier.hp, 
value: I32
): Web.Kaelin.Player
open player
  case modifier { 
    damage: 
      let damaged = I32.sub(player.current_hp, value)
      Web.Kaelin.Player.new(player.addr, player.team, damaged, player.current_status)
    heal: 
      let healed = I32.add(value, player.current_hp)
      Web.Kaelin.Player.new(player.addr, player.team, healed, player.current_status)
  }







