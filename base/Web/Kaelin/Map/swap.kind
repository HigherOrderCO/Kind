// put the entity in coordinate ca on cb
Web.Kaelin.Map.swap(
  idx: Nat, 
  ca: Web.Kaelin.Coord, 
  cb: Web.Kaelin.Coord,
  map: Web.Kaelin.Map
) : Web.Kaelin.Map
  let {map,ent} = Web.Kaelin.Map.pop_at(idx, ca, map)
  let map = case ent {
    none: map
    some: Web.Kaelin.Map.push(cb, [ent.value], map)
  }
  map