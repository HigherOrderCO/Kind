Web.Kaelin.Map.pop_at(
  idx: Nat, 
  coord: Web.Kaelin.Coord, 
  map: Web.Kaelin.Map
) : Pair<Web.Kaelin.Map, Maybe<Web.Kaelin.Entity>>
  let key = Web.Kaelin.Coord.Convert.axial_to_nat(coord)
  let tile = NatMap.get!(key, map)
  case tile {
    none:{map, none}
    some:
      let {ent, remaining_tile} = List.pop_at!(idx, tile.value)
      {(NatMap.set!(key, remaining_tile, map)), ent} 
  }
