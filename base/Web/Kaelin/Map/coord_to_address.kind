Web.Kaelin.Map.coord_to_address(
player_coord: Web.Kaelin.Coord,
map: Web.Kaelin.Map
): Maybe<String>
  tiles = Web.Kaelin.Map.get(player_coord, map)
  addr = none
  case tiles { 
    none: none
    some:
      for entity in tiles.value with addr:
        case entity {
          creature: 
            case entity.player { 
              none: addr
              some: some(entity.player.value)
          }
        }default addr
      addr
  }

