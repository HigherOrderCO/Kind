Web.Kaelin.Map.find_players_on_coords(coords: List(Web.Kaelin.Coord), map: Web.Kaelin.Map): List<Pair(String, Web.Kaelin.Coord)>
  player = ""
  players = []
  for coord in coords with players:
    let tile = Web.Kaelin.Map.get(coord,map) <> []
    for entity in tile with player:
      case entity{
        creature: 
          case entity.player{
            none: player
            some: entity.player.value
          }
      }default player
  {player, coord} & players
players