// Returns state with updated cast_info
Web.Kaelin.Action.update_interface(interface : App.EnvInfo, state: Web.Kaelin.State): Web.Kaelin.State
   case state {
     game: 
        Web.Kaelin.State.game(state.user, state.room, state.tick, state.players, state.cast_info, state.map, interface)
   } default state
