Web.Kaelin.Action.show_range(adress: String, range: Nat, state: Web.Kaelin.State): Web.Kaelin.State
case state{
    game:
        let player = state.players{adress}
        case player {
            none:state
            some:
            open player.value
            let new_map =  Web.Kaelin.Draw.show_range(player.value.coord, state.map, range)
            Web.Kaelin.State.game(state.room, state.tick, state.players, new_map, state.interface)
        }
        
} default state
