Web.ColD: App<Nat>
  img = VoxBox.alloc_capacity(65536#32)

  init = 0

  draw = (state)
    let img = VoxBox.Draw.square(10#32, 10#32, 0#32, 5#32, 5#32, (x,y) Col32.new(255#32,0#32,0#32,255#32), img)
    DOM.node("div", {}, {}, [
      DOM.text("Welcome to ColD :)")
      DOM.vbox({}, {}, img)
    ])

  when = (event, state)
    App.pass

  App.new!(init, draw, when)
