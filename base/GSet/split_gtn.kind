GSet.split_gtn<A: Type>(cmp: A -> A -> Cmp, cut: A, set: GSet<A>): GSet<A>
  case set {
    tip: set,
    bin: case cmp(cut, set.val) {
      ltn:  
        let left  = GSet.split_gtn!(cmp, cut, set.left)
        GSet.concat3!(cmp, set.val, left, set.right),
      eql: set.right,
      gtn: GSet.split_gtn!(cmp, cut, set.right)
    }
  }