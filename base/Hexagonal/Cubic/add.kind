// Returns the sum of two points given two coordinates
Hexagonal.Cubic.add(
  coord_a: Hexagonal.Cubic,
  coord_b: Hexagonal.Cubic
): Hexagonal.Cubic

  open coord_a
  open coord_b
  let x = coord_a.x + coord_b.x // I32
  let y = coord_a.y + coord_b.y // I32
  let z = coord_a.z + coord_b.z // I32
  Hexagonal.Cubic.new(x, y, z)
