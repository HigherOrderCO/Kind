// TestSuite for RLP encoding
Ether.RLP.test: IO<Unit>
  TestSuite.run(Ether.RLP.test.suite, 0)

// Tests from Ethereum's repository:
// https://github.com/ethereum/tests/blob/develop/RLPTests/rlptest.json
Ether.RLP.test.suite: TestSuite
  TestSuite.many("RLP", [
    Ether.RLP.test.emptystring
    Ether.RLP.test.bytestring00
    Ether.RLP.test.bytestring01
    Ether.RLP.test.bytestring7F
    Ether.RLP.test.shortstring
    Ether.RLP.test.shortstring2
    Ether.RLP.test.longstring
    Ether.RLP.test.longstring2
    Ether.RLP.test.zero
    Ether.RLP.test.smallint
    Ether.RLP.test.smallint2
    Ether.RLP.test.smallint3
    Ether.RLP.test.smallint4
    Ether.RLP.test.mediumint1
    Ether.RLP.test.mediumint2
    Ether.RLP.test.mediumint3
    Ether.RLP.test.mediumint4
    Ether.RLP.test.emptylist
    Ether.RLP.test.stringlist
    Ether.RLP.test.multilist
    Ether.RLP.test.shortListMax1
    Ether.RLP.test.longList1
    Ether.RLP.test.longList2
    Ether.RLP.test.listsoflists
    Ether.RLP.test.listsoflists2
    Ether.RLP.test.dictTest1
    Ether.RLP.test.bigint
    Ether.RLP.parser.test.suite
  ])

Ether.RLP.test.emptystring: TestSuite
  TestSuite.unit("emptystring", 
    Ether.RLP.test.builder(Ether.RLP.data([]), "80")
  )

Ether.RLP.test.bytestring00: TestSuite
  TestSuite.unit("bytestring00", 
    Ether.RLP.test.builder(Ether.RLP.data([0x00]), "00")
  )

Ether.RLP.test.bytestring01: TestSuite
  TestSuite.unit("bytestring01", 
    Ether.RLP.test.builder(Ether.RLP.data([0x01]), "01")
  )

Ether.RLP.test.bytestring7F: TestSuite
  TestSuite.unit("bytestring7F",
    Ether.RLP.test.builder(Ether.RLP.data([0x7F]), "7f")
  )

Ether.RLP.test.shortstring: TestSuite
  TestSuite.unit("shortstring",
    Ether.RLP.test.builder(Ether.RLP.data(Bytes.from_str("dog")), "83646f67")
  )

