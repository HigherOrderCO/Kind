 //log(Nat.show(Bits.length(Nat.to_bits(10))))
  let hello = Bits.read("")
  let xs = Ether.RLP.encode.bytes(
    Ether.RLP.Tree.list([
      Ether.RLP.Tree.list([
        Ether.RLP.Tree.tip(hello),
