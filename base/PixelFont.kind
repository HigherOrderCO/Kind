PixelFont: Type
  Map(Image3D)

// Adds an Image3D to a char on the map
PixelFont.set_img(char: Char, img: Image3D, map: PixelFont): PixelFont
  Map.set!(U16.show_hex(char), img, map)

// Get the image given a map
PixelFont.get_img(char: Char, map: PixelFont): Maybe(Image3D)
  case Map.get!(U16.show_hex(char), map) as got{
    none: Maybe.none<Image3D>
    some: Maybe.some<Image3D>(got.value)
  }
