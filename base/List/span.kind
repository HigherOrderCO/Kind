List.span<A: Type>(f: A -> Bool, xs: List(A)): Pair(List(A),List(A))
  case xs {
    nil : {List.nil!, List.nil!},
    cons: case f(xs.head) {
      true: 
        let spanned = List.span!(f, xs.tail)
        { List.cons!(xs.head, Pair.fst!!(spanned)), Pair.snd!!(spanned) }
      false: { List.nil!, xs } 
    }
  }
