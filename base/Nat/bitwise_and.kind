Nat.bitwise_and(x: Nat, y: Nat): Nat
  case Bool.or(Nat.eql(x, 0), Nat.eql(y, 0)){
  true : 0
  false: 
    let bit = Bool.if<Nat>(Bool.or(Nat.eql(Nat.mod(x, 2), 0), Nat.eql(Nat.mod(y, 2), 0)), 0, 1)
    let x = Nat.div(x, 2) 
    let y = Nat.div(y, 2)
    Nat.add(bit, Nat.mul(2, Nat.bitwise_and(x, y)))
  }